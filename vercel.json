{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["server/routes/index", "server/**"]
      },
      "outputDirectory": ".nuxt",
      "installCommand": "npm install",
      "developmentCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  ]
}
