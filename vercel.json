{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "outputDirectory": ".nuxt",
      "installCommand": "/node16/bin/node /vercel/path1/node_modules/fibers/build && npm install",
      "developmentCommand": "npm run dev",
      "buildCommand": "npm run build"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["server/routes/index", "server/**"]
      }
    }
  ]
}
