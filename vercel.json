{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [".nuxt/dist/server/server", ".nuxt/dist/server/**"],
        "internalServer": true
      }
    }
  ]
}
